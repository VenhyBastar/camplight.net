    },
    {
      "path": "/plasma/PageStyle",
      "root": "/content/style"
    },
    {
      "path":"/plasma/PageRender",
      "root": "/content"
    },
    {
      "path": "/plasma/PageLayoutRender",
      "root": "/content"
    },
    "/plasma/ErrorHandler"
  ]
}