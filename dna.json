{
  "membrane":[
    {
      "path": "/membrane/httpServer",
      "port": "8001",
      "staticFolder": "/content/public"
    }
  ],
  "plasma": [
    "/plasma/LogHandler",
    "/plasma/PageRouter",
    {
      "path": "/plasma/pages/IndexPageDataFetcher",
      "store": "/data/members.json"
    },
    {
      "path": "/plasma/PageCode",
