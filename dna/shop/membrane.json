{
  "HttpServer": {
    "routes": {
      "/shop": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop"
      },
      "/shop.js": {
        "chain": [ "PageCode", "HttpServer" ],
        "code": "/shop",
        "content-type": "text/javascript"
      },
      "/shop-request": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop-request"
      },
      "/shop-requestform": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop-requestform"
      }
    }
  }
}