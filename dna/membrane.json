{
  "HttpServer": {
    "source": "node_modules/organic-webcell/membrane/HttpServer",
    "port": 8080,
    "staticFolder": "/client/public",
    "routes": {
      "/" : {
        "chain": [ "LogicAction", "PageRender", "HttpServer"],
        "action": "/api/get-Members.js",
        "page": "/index"
      },
      "/code.js": {
        "chain": [ "PageCode", "HttpServer" ],
        "code": "/index.js",
        "content-type": "text/javascript"
      },

      "/about" : {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/about"
      },

      "/shop": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop"
      },
      "/shop.js": {
        "chain": [ "PageCode", "HttpServer" ],
        "code": "/shop.js",
        "content-type": "text/javascript"
      },
      "/shop-request": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop-request"
      },
      "/shop-requestform": {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/shop-requestform"
      }
    },

    "notfoundRoute":  {
        "chain": [ "PageRender", "HttpServer" ],
        "page": "/404",
        "statusCode": 404
      }
    }
}